<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" href="~/favicon.ico" />
    <title>Elish Duran</title>

    <!--CSS-->
    <link rel="stylesheet" href="~/js/validation/css/formValidation.css" />
    <link rel="stylesheet" href="~/css/font-icons/entypo/css/entypo.css" />
    <link rel="stylesheet" href="~/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/neon-core.css" />
    <link rel="stylesheet" href="~/css/neon-theme.css" />
    <link rel="stylesheet" href="~/css/skins/blue.css" />
    <link rel="stylesheet" href="~/css/neon-forms.css" />
    <link rel="stylesheet" href="~/css/custom.css" />
    <link rel="stylesheet" href="~/js/pace/pace.min.css" />

    <!--JS-->
    <script src="~/js/jQuery/jQuery-2.1.4.min.js"></script>
 
    <!--[if lt IE 9]><script src="~/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="~/js/html5shiv.min.js"></script>
        <script src="~/js/respond.min.js"></script>
    <![endif]-->


</head>
<body class="page-body" onload="history.go(1);">

    <div class="locked" style="display:none;"></div>
    <div class="page-container">

        <div class="sidebar-menu">
            <div class="sidebar-menu-inner">

                <header class="logo-env">

                    <!-- logo -->
                    <div class="logo">
                        <a href="Home"><img src="~/images/yo-modified.png" width="200" alt="" /></a>

                    </div>


                    <!-- open/close menu icon (do not remove if you want to enable menu on mobile devices) -->
                    <div class="sidebar-mobile-menu visible-xs">
                        <a href="#" class="with-animation">
                            <!-- add class "with-animation" to support animation -->
                            <i class="entypo-menu"></i>
                        </a>
                    </div>

                </header>
                <ul id="main-menu" class="main-menu">
               
                    <li class="has-sub">
                        <a href="#">
                            <i class="entypo-layout"></i>
                          
                            <span class="title">Catálogos</span>
                        </a>
                        <ul>
                            <li>
                                <a asp-controller="Empresa">
                                    <span class="title">Empresas</span>
                                </a>
                            </li>
                             
                        </ul>
                    </li>

                </ul>

            </div>
        </div>

        <div class="main-content">
            <div class="row">

                <!-- Profile Info and Notifications -->
                <div class="col-md-6 col-sm-8 clearfix hidden-xs">

                </div>

                <!-- Raw Links -->
                <div class="col-md-6 col-sm-4 clearfix ">
                    <ul class="list-inline links-list pull-right">
                        <!-- Profile Info -->
                        <li class="profile-info dropdown">

                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                Elish Duran
                                <img src="~/images/yo-modified.png" alt="" class="img-circle" width="42" />
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <br />

            @RenderBody()
        </div>
    </div>

    <div id='myModal' class='modal fade' data-backdrop="static">
        <div class="modal-dialog">
            <div class="modal-content">
                <div id="contenModal"></div>
            </div>
        </div>
    </div>

    <!-- Imported styles on this page -->
    <link rel="stylesheet" href="~/js/datatables/datatables.css" />
    <link rel="stylesheet" href="~/js/select2/css/select2.min.css" />
    <link rel="stylesheet" href="~/js/datepicker/jquery.datetimepicker.min.css" />
    <link rel="stylesheet" href="~/js/bootstrap-select/css/bootstrap-select.min.css" />

    <!-- JavaScripts -->
    <script data-pace-options='
            {
            "restartOnRequestAfter": true,
            "eventLag": false,
            "ajax": { "trackMethods": ["POST"] },
            "startOnPageLoad": true
            }'
            src="~/js/pace/pace.min.js">
    </script>

    <script src="~/js/gsap/TweenMax.min.js"></script>
    <script src="~/js/datatables/datatables.js"></script>
    <script src="~/js/select2/js/select2.full.min.js"></script>
    <script src="~/js/select2/js/i18n/es.js"></script>
    <script src="~/js/validation/js/formValidation.js"></script>
    <script src="~/js/validation/js/framework/bootstrap.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/joinable.js"></script>
    <script src="~/js/resizeable.js"></script>
    <script src="~/js/neon-api.js"></script>
    <script src="~/js/bootstrap-modal-mult.js"></script>
    <script src="~/js/jquery.inputmask.bundle.js"></script>
    <script src="~/js/datepicker/jquery.datetimepicker.full.min.js"></script>
    <script src="~/js/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/js/jquery.multi-select.js"></script>
    <script src="~/js/neon-custom.js"></script>
    <script src="~/js/toastr.js"></script>
    <script src="~/js/load-image.all.min.js"></script>
    <script src="~/js/jfunciones.js"></script>
    <script src="~/js/bootbox.min.js"></script>
    <script src="~/js/bootstrap-filestyle.min.js"></script>
    <script src="~/js/jspdf.min.js"></script>


    @RenderSection("scripts", required: false)
</body>
</html>